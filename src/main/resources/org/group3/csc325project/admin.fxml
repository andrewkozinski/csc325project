         </ImageView>
         <ImageView fx:id="account_button" fitHeight="450.0" fitWidth="120.0" layoutX="670.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../../../Images/general_button.png" />
         </ImageView>
         <Label fx:id="account_Name_label" layoutX="690.0" layoutY="15.0" prefHeight="24.0" prefWidth="75.0" textAlignment="CENTER" wrapText="true">
            <font>
               <Font name="Bell MT" size="18.0" />
            </font>
         </Label>
         <Label fx:id="account_button_hiitbox" layoutX="666.0" layoutY="3.0" onMouseClicked="#openAccountButton" prefHeight="56.0" prefWidth="130.0" textAlignment="CENTER" wrapText="true">
            <font>
               <Font name="Bell MT" size="18.0" />
            </font>
         </Label>
         <Label fx:id="welcomeBack2Label" layoutX="180.0" layoutY="129.0" prefHeight="209.0" prefWidth="563.0">
            <font>
               <Font name="Bell MT Bold" size="48.0" />
            </font>
         </Label>
         <Label fx:id="welcomeBackLabel" layoutX="187.0" layoutY="129.0" prefHeight="209.0" prefWidth="563.0">
            <font>
               <Font name="Bell MT Bold" size="48.0" />
            </font>
         </Label>
