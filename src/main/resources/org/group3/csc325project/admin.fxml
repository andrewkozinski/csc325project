      </ImageView>
      <ImageView fx:id="admin_title_background" fitHeight="450.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
         <Image url="@../../../Images/registrationSystem_header_no_user.png" />
      </ImageView>
      <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="450.0" prefWidth="800.0">
         <ImageView fx:id="admin_adminbackground" fitHeight="450.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../../../Images/general_menu_background.png" />
         </ImageView>
         <ImageView fx:id="admin_sidebar_background" fitHeight="450.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../../../Images/general_button_background.png" />
         </ImageView>
         <Button layoutX="454.0" layoutY="221.0" mnemonicParsing="false" onAction="#returnToCreateUser" text="create user" visible="false" />
