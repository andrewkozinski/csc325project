<VBox prefHeight="450.0" prefWidth="800.0" stylesheets="@../../../CSS/general.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.group3.csc325project.AdminController">
   <StackPane prefHeight="450.0" prefWidth="800.0">
      <ImageView fx:id="admin_background_image_view" fitHeight="450.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
